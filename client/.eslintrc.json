{
    "parser":  "@typescript-eslint/parser",
    "extends":  [
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions":  {
        "project": [
            "client/tsconfig.json"
        ],
        "ecmaVersion":  6,
        "sourceType":  "module",
        "ecmaFeatures":  {
            "jsx":  true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules":  {
    },
    "settings":  {
        "react":  {
            "version":  "detect"
        }
    }
}